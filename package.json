{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "new-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "bun@1.2.22",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"update:dep:latest": "ncu --target latest -ui --format group --install always --packageManager bun",
		"update:dep:newest": "ncu --target newest -ui --format group --install always --packageManager bun"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.5.2",
		"bits-ui": "^2.11.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsdoc": "^60.4.0",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"lucide-svelte": "^0.544.0",
		"npm-check-updates": "18.3.0",
		"prettier": "~3.6.2",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.6",
		"svelte-check": "^4.3.2",
		"svelte-moveable": "^0.45.0",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@faker-js/faker": "^10.0.0"
	}
}
