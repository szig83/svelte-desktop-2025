{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "new-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "bun@1.2.22",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:generate": "cross-env DB_MIGRATING=true drizzle-kit generate",
		"db:push": "cross-env DB_MIGRATING=true drizzle-kit push",
		"db:migrate": "cross-env DB_MIGRATING=true bun src/lib/server/database/migrate",
		"db:seed": "bun src/database/seed.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.562.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.3",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.3",
		"bits-ui": "^2.15.4",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsdoc": "^62.0.0",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"lucide-svelte": "^0.562.0",
		"mode-watcher": "^1.1.0",
		"prettier": "~3.7.4",
		"prettier-plugin-css-order": "^2.2.0",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"svelte-moveable": "^0.45.0",
		"svelte-sonner": "^1.0.7",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@faker-js/faker": "^10.2.0",
		"@types/pg": "^8.16.0",
		"better-auth": "^1.4.10",
		"clsx": "^2.1.1",
		"cross-env": "^10.1.0",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"drizzle-valibot": "^0.4.2",
		"html-to-image": "^1.11.13",
		"lz-string": "^1.5.0",
		"pg": "^8.16.3",
		"phosphor-svelte": "^3.0.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss-animate": "^1.0.7",
		"tw-animate-css": "^1.4.0",
		"valibot": "^1.2.0"
	}
}
